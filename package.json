{
	"name": "browser-links",
	"displayName": "browser-links",
	"description": "Browser Tabs as completions in VS Code's SCM input box",
	"publisher": "jrieken",
	"repository": {
		"url": "https://github.com/jrieken/vscode-browser-links"
	},
	"license": "MIT",
	"version": "0.0.9",
	"engines": {
		"vscode": "^1.58.0"
	},
	"categories": [
		"SCM Providers",
		"Other"
	],
	"activationEvents": [
		"onLanguage:scminput"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": [
			{
				"title": "browser-links",
				"properties": {
					"browser-links.pattern": {
						"type": "string",
						"markdownDescription": "Regular expression that is matched against URLs, e.g `https://github.com/.+/issues/.+`"
					},
					"browser-links.browser": {
						"type": "string",
						"enum": [
							"Safari",
							"Microsoft Edge",
							"Google Chrome"
						],
						"default": "Safari"
					}
				}
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"publish": "vsce publish --target darwin-x64 darwin-arm64",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/node": "^12.11.7",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.18.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"glob": "^7.1.6",
		"typescript": "^3.7.5"
	}
}
